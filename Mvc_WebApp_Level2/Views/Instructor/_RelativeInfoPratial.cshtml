@model List<Instructor> //id, Name, Image, Salary, Address, DeptId
@{

}
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>#Id</th>
            <th>Name</th>
            <th>Image</th>
            <th>Salary</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td><img src="~/Images/@item.Image" style="width:30px"/></td>
                <td>@item.Salary</td>
                <td>@item.Address</td>
            </tr>
        }
    </tbody>
</table>